{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "moduleResolution": "node",
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "target": "es5",
  }
}
