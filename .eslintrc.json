{
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "react-hooks"
  ],
  "extends": [
    "eslint:recommended",
    "airbnb"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "jsx-a11y/anchor-is-valid": 0
  }
}
