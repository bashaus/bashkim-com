{
  "name": "style-guide",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/style-guide/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/style-guide/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/style-guide/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
