{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "isolatedModules": true,
    "baseUrl": "./src",
    "typeRoots": ["./types"],
    "paths": {
      "@bashkim-com/prismic/*": ["../../prismic/build"],
      "%components/*": ["./components/*"],
      "%contexts/*": ["./contexts/*"],
      "%libraries/*": ["./libraries/*"],
      "%partials/*": ["./partials/*"],
      "%prismic/*": ["./libraries/prismic/*"],
      "%public/*": ["../public/*"],
      "%slices/*": ["./slices/*"],
      "%storybook/*": ["../.storybook/*"],
      "%styleguide/*": ["./styleguide/*"]
    },
    "jsx": "preserve"
  },
  "exclude": ["node_modules"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
