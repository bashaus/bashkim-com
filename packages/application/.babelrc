{
  "sourceMaps": true,

  "presets": ["next/babel"],

  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "%components": "./src/components",
        "%contexts": "./src/contexts",
        "%libraries": "./src/libraries",
        "%partials": "./src/partials",
        "%prismic": "./src/libraries/prismic",
        "%public": "./public",
        "%slices": "./src/slices",
        "%storybook": "./.storybook",
        "%styleguide": "./src/styleguide"
      }
    }]
  ],

  "env": {
    "production": {
      "sourceMaps": false
    }
  }
}
