{
  "sourceMaps": true,
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "%components": "./src/components",
          "%contexts": "./src/contexts",
          "%libraries": "./src/libraries",
          "%machines/*": "./src/machines/*",
          "%partials": "./src/partials",
          "%public": "./public",
          "%slices": "./src/slices",
          "%styleguide": "./src/styleguide"
        }
      }
    ]
  ],
  "env": {
    "production": {
      "sourceMaps": false
    }
  }
}
