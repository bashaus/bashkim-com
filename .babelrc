{
  "sourceMaps": true,

  "presets": ["next/babel"],

  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "%components": "./src/components",
          "%contexts": "./src/contexts",
          "%libraries": "./src/libraries",
          "%prismic": "./src/libraries/prismic",
          "%public": "./public",
          "%storybook": "./.storybook",
          "%styleguide": "./src/styleguide"
        }
      }
    ]
  ],

  "env": {
    "production": {
      "sourceMaps": false
    }
  }
}
