{
  "name": "prismic-schema",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/prismic-schema/src",
  "projectType": "application",
  "targets": {
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["ts-node apps/prismic-schema/src/main.ts"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/prismic-schema/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/prismic-schema/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
