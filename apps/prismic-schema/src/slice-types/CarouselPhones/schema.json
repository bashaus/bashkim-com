{
  "type": "Slice",
  "fieldset": "Carousel of Phones",
  "description": "Display mobile screens in a carousel",
  "icon": "phone_android",
  "display": "grid",
  "non-repeat": {},
  "repeat": {
    "carousel_phones_slice_type_device_type": {
      "type": "Select",
      "config": {
        "options": ["FEATURE_PHONE", "SMARTPHONE"],
        "default_value": "SMARTPHONE",
        "label": "Device Type"
      }
    },
    "carousel_phones_slice_type_image": {
      "type": "Image",
      "config": {
        "constraint": {},
        "thumbnails": [],
        "label": "Image"
      }
    },
    "carousel_phones_slice_type_caption": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph,preformatted,heading3,strong,em,hyperlink,image,embed,list-item,o-list-item,o-list-item",
        "label": "Caption"
      }
    }
  }
}
